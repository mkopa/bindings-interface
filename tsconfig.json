{
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "ESNext",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    // "moduleResolution": "ESNext",
    "noEmit": true,
    "allowJs": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "declaration": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": false
  },
  "include": [
    "lib/**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
